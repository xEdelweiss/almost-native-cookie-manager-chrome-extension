{
    "name": "Almost Native Cookies Manager",
    "version": "0.5.0",
    "manifest_version": 2,
    "description": "Look for me in devtools.",
    "devtools_page": "devtools.html",
    "background": {
        "scripts": ["js/background.js"]
    },
    "permissions": [
        "tabs",
        "cookies",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    }
}