{
  "name": "Almost Native Cookies Manager",
  "version": "0.3.0",
  "manifest_version": 2,
  "description": "Look for me in devtools.",
  "devtools_page": "devtools.html",
  "background": { "scripts": ["js/background.js"] },
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*"
  ]
}